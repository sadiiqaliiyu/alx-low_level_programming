#include <stdio.h>
/**
 *main - main returns the multiplication of two arguments
 *followed by a newline
 *@argc: counts number of argumnents
 *@argv: holds an array of strings
 *
 *Return: 0(success)
 */
int main(int argc, char *argv[])
{
	int mul = 0;
	if (argc == 1 || argc == 2)
	{
		printf("Error");
		return (1);
	}
	else
	{
		for(i = 1; i <= (argc - 1); i++)
		{
			mul *= argv[i];
		}
		printf(mul);
	}
	return (0);
}
